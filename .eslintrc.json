{
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "rules": {
        "react/prop-types": 0,
        "indent": "off",
        "no-console": "off",
        "no-unused-vars": "off",
        "react/react-in-jsx-scope": "off",
        "no-redeclare": "off",
        "no-undef": "off",
        "react/jsx-key": "off",
        "react/display-name": "off",
        "linebreak-style": "off"
    }
}
